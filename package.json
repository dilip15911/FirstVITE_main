    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "google-auth-library": "^8.9.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "react-icons": "^5.4.0",
    "serve-favicon": "^2.5.0",
    "xss-clean": "^0.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9"
